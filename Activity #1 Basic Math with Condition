public class Main {
  public static void main(String[] args) {
    int a = 10, b = 5;       // Gumawa ng dalawang number: a = 10 at b = 5
    int sum = a + b;         // Idagdag ang a at b, ilagay ang sagot sa "sum"

    System.out.println("The sum is: " + sum); // Ipakita ang resulta ng addition

    if (sum > 10) {          // Kung ang sagot ay mas malaki sa 10
      System.out.println("The sum is greater than 10.");
    } else {                 // Kung hindi mas malaki sa 10
      System.out.println("The sum is not greater than 10.");
    }
  }
}
